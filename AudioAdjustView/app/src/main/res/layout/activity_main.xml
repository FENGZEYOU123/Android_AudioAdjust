<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <com.yfz.main.View.IosColumnAudioView
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:iosColumnAudioView_setColorLoud="@color/colorLoud"
        app:iosColumnAudioView_setColorBackground="@color/colorBackground"
        app:iosColumnAudioView_setRadiusXY="@dimen/ios_column_audio_view_radius"
        android:layout_width="50dp"
        android:layout_height="200dp">
    </com.yfz.main.View.IosColumnAudioView>

</androidx.constraintlayout.widget.ConstraintLayout>